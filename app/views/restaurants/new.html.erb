


<% provide(:title, 'New Restaurant') %>

<h1>New Restaurant</h1>


<div class="row">
  <div class="span6 offset3">

    <%#= @map.name %>, <%#= @map.file_name %>


    <%= form_for(@restaurant) do |f| %>
      
      <%= f.label :name,                  "Name" %>
      <%= f.text_field :name %>
      
      <%= f.label :info,                  "Info" %>
      <%= f.text_field :info %>
      
      <%= f.label :lat_dec,                  "Lat decimal" %>
      <%= f.text_field :lat_dec %>
      
      <%= f.label :lng_dec,                  "Long decimal" %>
      <%= f.text_field :lng_dec %>
      


      <%#= f.label :file_name,              "File name" %>
      <%#= f.text_field :file_name %>

      <%#= f.label :type,                 "Type" %>
      <%#= f.text_field :type %>


      <%= f.submit "Create restaurant", class: "btn btn-large btn-primary" %>
    <% end %>
    

  </div>
</div>

