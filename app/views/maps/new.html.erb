

<% provide(:title, 'New Map') %>
<h3>New Map</h3>


<div class="row">
  <div class="span6 offset3">

    <%#= @map.name %>, <%#= @map.file_name %>


    <%= form_for(@map) do |f| %>
      
      <%= f.label :name,                  "Name" %>
      <%= f.text_field :name %>
      
      <%= f.label :map_type,                  "Map type" %>
      <%= f.text_field :map_type %>
      
      <%= f.label :created_by,                  "Created by" %>
      <%= f.text_field :created_by %>
      
      <%= f.label :last_updated_by,                  "Last updated by" %>
      <%= f.text_field :last_updated_by %>
      
      <%= f.label :comments,                  "Comments" %>
      <%= f.text_field :comments %>
      


      <%= f.submit "Create map", class: "btn btn-large btn-primary" %>
    <% end %>
    

  </div>
</div>

